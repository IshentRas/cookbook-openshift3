ClusterResourceOverride:   
  configuration:
    apiVersion: v1
    kind: ClusterResourceOverrideConfig
<% unless node['is_apaas_openshift_cookbook']['openshift_clusterresourceoverrides_memorylimit_percent'].empty? %>
    memoryRequestToLimitPercent: <%= node['is_apaas_openshift_cookbook']['openshift_clusterresourceoverrides_memorylimit_percent'] %>
<%- end -%>
<% unless node['is_apaas_openshift_cookbook']['openshift_clusterresourceoverrides_cpulimit_percent'].empty? %>
    cpuRequestToLimitPercent: <%= node['is_apaas_openshift_cookbook']['openshift_clusterresourceoverrides_cpulimit_percent'] %>
<%- end -%>
<% unless node['is_apaas_openshift_cookbook']['openshift_clusterresourceoverrides_limitcputomem_percent'].empty? %>
    limitCPUToMemoryPercent: <%= node['is_apaas_openshift_cookbook']['openshift_clusterresourceoverrides_limitcputomem_percent'] %>
<%- end -%>
