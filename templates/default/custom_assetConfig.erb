  <% if node['is_apaas_openshift_cookbook']['openshift_master_asset_config'] %>
  <% for key in ['extensionScripts' , 'extensionStylesheets'] %>
  <% if node['is_apaas_openshift_cookbook']['openshift_master_asset_config'].has_key?(key) %>
  <%= key %>:
  <%- node['is_apaas_openshift_cookbook']['openshift_master_asset_config'][key].each do |name| -%>
    - <%= name %>
  <% end %>
  <% end %>
  <% end %>
  <% if node['is_apaas_openshift_cookbook']['openshift_master_asset_config'].has_key?('extensions') %>
  extensions:
    - name: images
      sourceDirectory: <%= node['is_apaas_openshift_cookbook']['openshift_master_asset_config']['extensions'] %>
  <% end %>
  <% end %>
